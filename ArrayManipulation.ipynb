    "n, m = map(int, input().split())\n",
    "a = [0]*(n+1)\n",
    "m_val = temp = 0\n",
    "for _ in range(m):\n",
    "    f, l, val = [int(n) for n in input().split()]\n",
    "    a[f-1] += val\n",
    "    if l < n:\n",
    "        a[l] -= val\n",
    "    print(a)\n",
    "for i in a:\n",
    "    temp += i\n",
    "    if m_val < temp:\n",
    "        m_val = temp\n",
    "print(m_val)"
   ]
